{
  "version": 2,
  "routes": [
    {
      "src": "/calculadora.php/**",
      "dest": "/calculadora.php",
      "headers": { "Content-Type": "text/html" }
    }
  ],
  "builds": [
    { "src": "calculadora.php", "use": "@vercel/build-php", "config": { "expose": ["public"] }, "publish": true }
  ]
}
